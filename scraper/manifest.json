{
    "manifest_version": 3,
    "name": "Sudoku Suggest",
    "version": "1.0",
    "permissions": ["activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon64.png",
        "128": "images/icon128.png"
      }
    }
  }
  